.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.text-over{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.flex{
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.highlight-light{
  color: darkgrey;
}
.highlight-heavy{
  color: deepskyblue;
}

